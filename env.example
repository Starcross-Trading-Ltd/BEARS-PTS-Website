# EmailJS Configuration
# Replace these values with your actual EmailJS credentials

# EmailJS Service ID (found in your EmailJS dashboard under Email Services)
VITE_EMAILJS_SERVICE_ID=your_service_id_here

# EmailJS Template IDs (found in your EmailJS dashboard under Email Templates)
VITE_EMAILJS_CONTACT_TEMPLATE_ID=your_contact_template_id_here
VITE_EMAILJS_BOOKING_TEMPLATE_ID=your_booking_template_id_here
VITE_EMAILJS_CAREER_TEMPLATE_ID=your_career_template_id_here

# EmailJS Public Key (found in your EmailJS dashboard under Account > API Keys)
VITE_EMAILJS_PUBLIC_KEY=your_public_key_here

# Example values (replace with your actual values):
# VITE_EMAILJS_SERVICE_ID=service_abc123
# VITE_EMAILJS_CONTACT_TEMPLATE_ID=template_xyz789
# VITE_EMAILJS_BOOKING_TEMPLATE_ID=template_def456
# VITE_EMAILJS_CAREER_TEMPLATE_ID=template_ghi789
# VITE_EMAILJS_PUBLIC_KEY=user_jkl012 